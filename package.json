    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "moment": "^2.25.3",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
